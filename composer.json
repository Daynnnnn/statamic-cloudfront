{
    "name": "daynnnnn/statamic-cloudfront",
    "description": "Allows you to use cloudfront as a statamic cache strategy.",
    "autoload": {
        "psr-4": {
            "Daynnnnn\\Statamic\\Cloudfront\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Cloudfront",
            "description": "Allows you to use cloudfront as a statamic cache strategy."
        },
        "laravel": {
            "providers": [
                "Daynnnnn\\Statamic\\Cloudfront\\CloudfrontServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "^5.0",
        "aws/aws-sdk-php": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.89",
        "laravel/pint": "^1.20"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true,
            "pestphp/pest-plugin": true
        }
    }
}
